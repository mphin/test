#!name = Google重定向
#!desc = 将google.cn/google.com.hk重定向回google.com
#!date = 2024-02-29 18:48:16
*-google-cn, *-google-com-hk#!date 00:00:00

[Rewrite]
^https?:\/\/(www.)?(g|google)\.cn https://www.google.com 302
^https?:\/\/(www.)?(g|google)\.com.hk https://www.google.com 302

[Mitm]
hostname = hostname = *.google.cn, *.google.com.hk#!date = 2024-02-29 18:48:16
